<h1>Me contacter</h1>
<form method="post" action="contact-post">
    <div>
        <label for="email">Votre email: </label>
        <input type="email" name="email" placeholder="Votre email...">
    </div>
    <div>
        <label for="title_contact">Titre de votre message: </label>
        <input type="text" name="title_contact" placeholder="Titre de votre message ...">
    </div>
    <div>
        <label for="textarea_contact">Votre message: </label>
        <textarea name="textarea_contact" id="textarea" cols="30" rows="10" placeholder="Message..."></textarea>
    </div>
    <button type="submit" class="btn btn-success">Envoyer</button>
</form>
<form action="/">
    <button type="submit" class="btn btn-primary btn-sm" onclick="contact()">Retour à l'accueil</button>
</form>
<script>
        function contact(){
        var message = "Message envoyé"
        alert(message);
        }
</script>
